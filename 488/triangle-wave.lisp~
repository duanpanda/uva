(defparameter *amplitude* 3)
(defparameter *frequency* 2)

(dotimes (x *frequency*)
  (dotimes (y *amplitude*)
    (dotimes (z (1+ y))
;      (format t "x=~a, y=~a, z=~a~%" x y z)
      (format t "~a" (1+ y)))
    (format t "~%"))
  (dotimes (y *amplitude*)
    (dotimes (z (- *amplitude* y 1))
;      (format t "x=~a, y=~a, z=~a~%" x y z)
      (format t "~a" (- *amplitude* y 1)))
    (format t "~%"))
;  (if (< x (1- *frequency*))
;      (format t "~%"))
  'done)